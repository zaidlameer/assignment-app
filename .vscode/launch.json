{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/customerManager/manage.py",
            "args": [
                "runserver",
            ],
            "django": true,
            "justMyCode": true,
            "console": "integratedTerminal",
            "env": {
                "DJANGO_SETTINGS_MODULE": "customerManager.settings",
                "PYTHONUNBUFFERED": "1"
            }
        }
    ]
}
